{
	"name": "Clipboard",
	"description": "Keep track of your clipboard history",
	"version": "1.0",
	"manifest_version": 3,
	"action": {
		"default_popup": "popup.html"
	},
	"permissions": ["clipboardRead", "commands", "storage"],
	"commands": {
		"_execute_action": {
			"suggested_key": {
				"default": "Ctrl+Shift+V"
			}
		}
	}
}
